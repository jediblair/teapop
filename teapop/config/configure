if test "${enable_lock+set}" = set; then
  enableval="$enable_lock"
  pop_lock="$enableval"
else
  pop_lock="flock"
fi


echo $pop_lock | sed -e "s/,/\\
/g" | while read line;
do
	if test x"$line" = x"dotlock"; then
		cat >> confdefs.h <<\EOF
#define DOTLOCK 1
EOF

	else
		if test x"$line" = x"fcntl"; then
			cat >> confdefs.h <<\EOF
#define FCNTL 1
EOF

		else
			if test x"$line" = x"flock"; then
				cat >> confdefs.h <<\EOF
#define FLOCK 1
EOF

			else
				if test x"$line" = x"lockf"; then
					cat >> confdefs.h <<\EOF
#define LOCKF 1
EOF

				else
					if test x"$line" = x""; then
						:;
					else
						{ echo "configure: error: unknown method: $line" 1>&2; exit 1; }
					fi
				fi
			fi
		fi
	fi
done

if test $? -gt 0; then
	exit 1
fi

if test x"$foundlock" = x"0"; then
	{ echo "configure: error: you must specify a method" 1>&2; exit 1; }
else
	echo "$ac_t""$pop_lock" 1>&6
fi

